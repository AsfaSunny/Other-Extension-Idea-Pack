{
    "manifest_version": 2,
    "name": "Dark Theme Extension",
    "version": "1.0",
    "description": "A simple Chrome extension that applies a dark theme to the current website",
    "permissions": [
      "activeTab"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }
  